{
  "name": "@riddledc/openclaw-riddledc",
  "version": "0.7.0",
  "description": "OpenClaw integration package for RiddleDC (no secrets).",
  "license": "MIT",
  "author": "RiddleDC",
  "repository": {
    "type": "git",
    "url": "https://github.com/riddledc/integrations",
    "directory": "packages/openclaw-riddledc"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "openclaw.plugin.json",
    "CHECKSUMS.txt",
    "SECURITY.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=18"
  },
  "openclaw": {
    "extensions": [
      "."
    ]
  },
  "scripts": {
    "build": "npm run sync:openclaw-plugin-version && tsup src/index.ts --format cjs,esm --dts --out-dir dist",
    "clean": "rm -rf dist",
    "lint": "echo 'lint: (not configured)'",
    "test": "echo 'test: (not configured)'",
    "sync:openclaw-plugin-version": "node scripts/sync-openclaw-plugin-version.mjs",
    "checksums": "node scripts/generate-checksums.mjs",
    "prepack": "npm run sync:openclaw-plugin-version && npm run checksums"
  },
  "dependencies": {
    "@sinclair/typebox": "^0.32.0"
  },
  "devDependencies": {
    "@types/node": "^25.3.0",
    "tsup": "^8.0.1",
    "typescript": "^5.4.5"
  }
}
